
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <p id="notice"><%= notice %></p>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
    <h1 class="h1"><%= @co_operation.title %></h1>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <blockquote>
        <p><%= @co_operation.column_1 %></p>
      </blockquote>
      <blockquote>
        <p><%= @co_operation.column_2 %></p>
      </blockquote>
      <blockquote>
        <%= image_tag @co_operation.image, width: 250 %>
      </blockquote>
    </div>
  </div>

<div class="btn-group">
  <%= link_to 'Edit', edit_co_operation_path(@co_operation), class: "btn btn-primary" %>
</div>


